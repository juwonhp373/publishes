							.category-card.type-image
								.category-card-cover
								.text-group
									.name IMAGE
									.desc-group
										.count 300
										.capacity / 38.35GB
								.icon-group
									.i.fas.fa-image.type-image-color
						.category-card-outer(type='deselected')
							.category-card.type-code
								.category-card-cover
								.text-group
									.name APPLICATION
									.desc-group
										.count 300
										.capacity / 38.35GB
								.icon-group
									.i.fas.fa-code.type-code-color
						.category-card-outer
							.category-card.type-text
								.category-card-cover
								.text-group
									.name TEXT
									.desc-group
										.count 300
										.capacity / 38.35GB
								.icon-group
									.i.fas.fa-font.type-text-color
